        "targets": [
          {
            "editorMode": "code",
            "expr": "quantile by (endpoint, country_code) (0.95, hls_playlist_chain_load_time{\n    continent=~\"$continent\",\n    country_code=~\"$country\",\n    city=~\"$city\",\n    isp=~\"$isp\",\n    endpoint=~\"$endpoint\"\n})[$__range]",
            "legendFormat": "{{country_code}} - {{endpoint}} Playlist Chain Load ",
            "range": true,
            "refId": "Chained"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "editorMode": "code",
            "expr": "quantile by (endpoint, country_code) (0.95, hls_master_tcp_connect_ms{\n    continent=~\"$continent\",\n    country_code=~\"$country\",\n    city=~\"$city\",\n    isp=~\"$isp\",\n    endpoint=~\"$endpoint\"\n})[$__range]",
            "hide": false,
            "instant": false,
